{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "practiceDataFactory10"
		},
		"2ndAzureDataLakeStorageconnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of '2ndAzureDataLakeStorageconnection'"
		},
		"AzureDataLakeStorageConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageConnection'"
		},
		"AzureSqlDatabaseConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseConnection'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=practicedb10.database.windows.net;Initial Catalog=practicedb;User ID=sqladmin"
		},
		"OnPremSqlServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremSqlServer'"
		},
		"2ndAzureDataLakeStorageconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://2ndpracticedatalake10.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://practicedatalake10.dfs.core.windows.net/"
		},
		"AzureKeyVaultconnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://practicekeyvault100.vault.azure.net/"
		},
		"UpdatedAzureKeyVaultConnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://practicekeyvault100.vault.azure.net/"
		},
		"OnPremSqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "LAPTOP-NIA8JHJN\\SQLEXPRESS"
		},
		"OnPremSqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sportsDB"
		},
		"OnPremSqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "superadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "practiceDataFactory10_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"2ndAzureDataLakeStorageconnection_accountKey": {
						"value": "[parameters('2ndAzureDataLakeStorageconnection_accountKey')]"
					},
					"AzureDataLakeStorageConnection_accountKey": {
						"value": "[parameters('AzureDataLakeStorageConnection_accountKey')]"
					},
					"2ndAzureDataLakeStorageconnection_properties_typeProperties_url": {
						"value": "[parameters('2ndAzureDataLakeStorageconnection_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageConnection_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageConnection_properties_typeProperties_url')]"
					},
					"AzureKeyVaultconnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultconnection_properties_typeProperties_baseUrl')]"
					},
					"UpdatedAzureKeyVaultConnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('UpdatedAzureKeyVaultConnection_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "practiceDataFactory10_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseConnection_connectionString": {
						"value": "[parameters('AzureSqlDatabaseConnection_connectionString')]"
					},
					"OnPremSqlServer_password": {
						"value": "[parameters('OnPremSqlServer_password')]"
					},
					"OnPremSqlServer_properties_typeProperties_server": {
						"value": "[parameters('OnPremSqlServer_properties_typeProperties_server')]"
					},
					"OnPremSqlServer_properties_typeProperties_database": {
						"value": "[parameters('OnPremSqlServer_properties_typeProperties_database')]"
					},
					"OnPremSqlServer_properties_typeProperties_userName": {
						"value": "[parameters('OnPremSqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'practiceDataFactory10_ArmTemplate_0')]"
			]
		}
	]
}